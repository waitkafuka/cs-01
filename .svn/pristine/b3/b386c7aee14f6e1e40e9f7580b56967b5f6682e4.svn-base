.pro-title-left,.pro-des-box{
    width:865px;
}
.pro-des-box{
    border: 1px solid #e7e7e7;
    min-height: 280px;
    padding-top:29px;
    padding-left: 25px;
    position: relative;
    padding-bottom: 39px;
}
.pro-get-button-box,.main-right-box{
    width:315px;
    margin-left:20px;
    text-align: center;
}
.pro-get-button-box{
    margin-left: 18px;
}
.main-right-box{
    min-height: 600px;
    background-color: #FAFAFA;
}
.main-right-box,.pro-des-box{
    margin-top:20px;
}
.pro-img-box{
    width: 122px;
    height: 122px;
    border: 1px solid #E7E7E7;
    padding:1px;
}
.pro-img-box img{
    width: 118px;
    height: 118px;
}
.pro-title-box{
    margin-top:20px;
    width:100%;
    height: 103px;
    border: 1px solid #E6E6E6;
}
.pro-title h4{
    margin-top:22px;
}
.pro-title,.pro-des{
    margin-left: 13px;
    /**float: left;影响图片的CSS**/
}
.pro-des{
    margin-top:15px;
}
.pro-des tr{
    height: 23px;
}
.get-button{
    width: 113px;
    height: 33px;
    background: #FEE145;
    font-size: 14px;
    color: white;
    border: none;
    margin-top:3px;
}
.detail-title{
    font-size: 14px;
    width: 85px;
    text-align: center;
    padding-bottom: 4px;
    border-bottom: 2px solid #0085D0;
}
.detail-title h4{
    color: black;
}
.detail-item-01{
    margin:10px;
    color:#A2A2A2;
}
.detail-item-01 tr{
    height:30px;
}
.detail-item-01 td{
    padding:5px;
}
.detail-item-01 .t1{
    width:432px;
}
.detail-item-01 .t2{
    width: 120px;
}
.pro-item-title{
    position: relative;
    margin-top:20px;
    height: 40px;
}
.pro-item-02-title,.pro-line-01,.spread-button{
    position: absolute;
}
.pro-item-02-title{
    font-size: 14px;
    font-weight: 600;
    left:15px;
}
.pro-line-01{
    width: 655px;
    height: 1px;
    display: inline-block;
    border-top: 1px solid #E6E6E6;
    left:83px;
    top:10px;
}
.spread-button{
    left:750px;
    top:-4px;
    display: inline-block;
    width: 70px;
    height: 29px;
    text-align: center;
    padding:5px;
}
.spread-button img{
    width: 10px;
}
.product-pre-box{
    height: 152px;
}
.product-des-box{
    margin-top:10px;
    margin-left:50px;
}
.product-des-box dt,.product-des-box dd{
    height:28px;
}
.detail-01 table{
    border-bottom: 1px dotted #C3C3C3;
}
.item-box-01{
    padding-left:23px;
}
.item-title{
    font-weight: bold;
    margin-top:22px;
}
.detail-des{
    margin-top:10px;
    width: 736px;
}
.detail-des tr{
    height:32px;
}
.detail-des .t1{
    width:276px;
}
.detail-des .t2{
    width:550px;
}
.connect-type{
    width: 276px;
    min-height: 152px;
    margin-left:5px;
    padding-top:30px;
    padding-left: 12px;
    text-align: center;
}
.connect-type h4{
    text-align: left;
    border-bottom: 2px solid #DFDFDF;
    padding-bottom: 8px;
}
.m-r-01{
    font-size:16px;
    margin-top:39px;
}
.m-r-02{
    color: #FD9149;
    font-size: 20px;
    margin-top:19px;
}
.m-r-02 img{
    margin-bottom: 5px;
}
.m-r-03{
    font-size: 14px;
    margin-top:5px;
}
.questions-list{
    margin-top:23px;
    text-align: left;
}
.questions-list li{
    height: 29px;
}
/**我的任务列表页面的CSS**/
#left-box,#right-box{
    margin-top:22px;
}
.tool-bar{
    height: 42px;
    background: #F5F5F5;
    width: 100%;
    padding-left: 14px;
}
.job-item{
    width: 100%;
    height: 140px;
    border-bottom: 1px dotted #DEDEDE;
    margin-top:20px;
}
.content-box .t2{
    width: 341px;
}
.content-box .t3,.content-box .t4{
    padding-left: 10px;
}
.job-state-img{
    float: right;
    width: 72px;
}
.fin-detail,.test-nec{
    width: 700px;
}
.fin-detail td,.test-nec td,.pro-des-box th{
    border: 1px solid #E6E6E6;
    text-align: center;
}
.test-nec tr,.fin-detail tr{
    height: 43px;
}
.pro-des-box th{
    background: #fafafa;
    height: 43px;
}
.test-nec{
    margin-top:20px;
}
.pass{
    color: #017CFE;
}
.rej{
    color: red;
}
.job-detail-item{
    margin-top:20px;
    float: left;
}
.job-detail-item h5{
    font-size: 14px;
    font-weight: 600;
    border-bottom: 2px solid #2FAD92;
    padding-bottom: 4px;
    width: 106px;
    text-align: center;
}
.job-content-box{
    width: 865px;
    border: 1px solid #E7E7E7;
    min-height:60px;
}
.job-content-box ul{
    margin-left:17px;
    border-bottom: 1px dashed #D6D6D6;
    padding-bottom: 20px;
}
.job-content-box li{
    height: 22px;
}
.call-record{
    margin-left: 16px;
    margin-bottom: 46px;
    width: 829px;
}
.call-record tr{
    height: 39px;
}
.call-record td,.call-record th{
    text-align: center;
    border: 1px solid #E6E6E6;
}
.call-record .t1{
    color: #FF8904;
}