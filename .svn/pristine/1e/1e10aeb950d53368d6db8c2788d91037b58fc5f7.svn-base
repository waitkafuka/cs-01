@charset "utf-8";
/* CSS Document */
body{
	padding:0;
	margin:0;
	font-family: "Microsoft Yahei","Helvetica Neue",Helvetica,Arial,Tahoma,sans-serif;
	font-size:12px;
	color:#5a5a5a;
	
	}
p {
  display: block;
  -webkit-margin-before: 1em;
  -webkit-margin-after: 1em;
  -webkit-margin-start: 0px;
  -webkit-margin-end: 0px;
}
img{
	border:none;
	}
ul{
	list-style:none;
	}
a{
	text-decoration:none;
	}
.clearfix{
	clear:both;
	}
.contain{
	width:1200px;
	height:1000px;
	margin:0 auto;
	border:#dadada 1px solid;
	}
.left{
	width:190px;
	height:1000px;
	background:#fafafa;
	/*height: 53px;*/
	float:left;
	display: block;
	}
.title{
	  height: 53px;
	  width: 100%;
	  border-bottom:1px #dadada solid;
	  text-indent: 24px;
	  line-height: 53px;
	  text-align: left;
	  font-size: 14px;
	  font-weight: bold;
  }
.settingTab {
	  width: 100%;
	 /* height:500px;*/
	  border: 0;
	  line-height: 53px;
	  text-align: left;
	  text-indent: 24px;
	  font-size: 14px;
	}
.settingTab-li {
	  border: none;
	  width:100%;
	  height: 22px;
	  line-height: 22px;
	  margin-top: 14px;
	  padding-left: 1px;
	  /*text-indent: 55px*/
	  list-style: outside none;
	}
	
/*<!------------------------------------  E-left	----------------------------------------------->*/
.right {
  width: 1000px;
  float: right;
  display: block;
  /*min-height: 582px;*/
  margin: 0;
  padding: 0;
  background: #fff;
 /* border-left: 1px #dadada solid;*/
 
}
#account {
  padding: 0 35px;
}
.account_title {
	font-size:18px;
  padding: 22px 0 8px;
  border-bottom: 1px solid #e6e6e6;
  line-height: 24px;
}
.spacer8 {
  height: 8px;
}

.account_myinfo {
  padding: 10px;
  background: #f5f5f5;
  margin-bottom: 28px;
}
.account_myinfo .imgbox img {
  width: 66px;
  height: 66px;
}
.floatleft {
  float: left;
}
.account_myinfo .infobox {
  padding-left: 15px;
}
.account_myinfo .infobox h1.name_gen {
  height: 28px;
  line-height: 28px;
  font-size: 18px;
  margin-bottom: 15px;
  display: inline-block;
  color: #000;
}
.pdlr4 {
  padding: 0 4px;
}
.account_iconsign_cheng{
  display: inline-block;
  width: 16px;
  height: 13px;
  background: url(../img/fund_01.png) no-repeat -64px -48px;
}

.account_iconsign_realname_gray {
  display: inline-block;
  width: 16px;
  height: 13px;
  background:url(../img/fund_01.png) no-repeat -64px -48px;
}
.account_iconsign_realname_gray {
  background-position: -64px -87px;
}
.kicon_level {
  width: 29px;
  height: 14px;
  display: inline-block;
  background-image: url(kicon_level.png);
  background-repeat: no-repeat;
}
.floatright{
	float:right;
	}
.kbtn30 {
  height: 30px;
  line-height: 30px;
}
.kbtn_gray {
  color: #fff;
  background: #29ab8f;
  border-radius: 3px;
}
.kbtn {
  display: inline-block;
  padding: 0 1em;
  border-radius: 2px;
  text-align: center;
  border: 0;
  cursor: pointer;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -o-box-sizing: border-box;
  box-sizing: border-box;
}







